{
  "name": "book-search",
  "version": "1.0.0",
  "description": "Google Books Search App.",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ManuelCoronaR/Book-Search.git"
  },
  "keywords": [],
  "author": "Manuel Corona",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ManuelCoronaR/Book-Search/issues"
  },
  "homepage": "https://github.com/ManuelCoronaR/Book-Search#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.20.1",
    "concurrently": "^5.1.0",
    "express": "^4.18.2",
    "if-env": "^1.0.4",
    "mongoose": "^5.9.3",
    "nodemon": "^2.0.2",
    "path": "^0.12.7"
  },
  "devDependencies": {}
}
